{

  "name":"Pomodoro Timer",
  "version":"1.0.0",
  "manifest_version":2,
  "description": "A Pomodoro timer with website blocking functionality.",
"permissions": [
  "notifications",
  "webRequest",
  "webRequestBlocking",
  "http://*/*",
  "https://*/*", "storage"
],
"background": {
  "scripts": ["background.js"],
  "persistent": true
},
"browser_action": {
  "default_popup": "popup.html"
},
"web_accessible_resources": ["blocked.html"],
"icons": {
  "16": "logo.png",
  "48": "logo.png",
  "128": "logo.png"
}
}


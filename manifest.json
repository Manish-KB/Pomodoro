{

    "name":"Pomodoro Timer",
    "version":"1.0.0",
    "manifest_version":2,
    "description": "A Pomodoro timer with website blocking functionality.",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}